@sql.offlineData.retrieveCategory2Code
SELECT a.CATEGORY_ID AS code,
       a.CATEGORY_NM AS code_nm ,
       a.PARENT_ID AS p_code
FROM RNDCALL_CATEGORY a
WHERE a.TREE_LEVEL = '2'
	  AND a.ISUSE = 'Y'
ORDER BY  a.CATEGORY_ID